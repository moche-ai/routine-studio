{
  "name": "Qwen Image Edit with LoRA",
  "description": "Qwen 이미지 편집 + 멀티앵글 LoRA",
  "workflow": {
    "1": {
      "class_type": "ETN_LoadImageBase64",
      "inputs": {
        "image": "PLACEHOLDER_IMAGE"
      }
    },
    "2": {
      "class_type": "VAELoader",
      "inputs": {
        "vae_name": "qwen_image_vae.safetensors"
      }
    },
    "3": {
      "class_type": "UNETLoader",
      "inputs": {
        "unet_name": "qwen_image_edit_2511_fp8mixed.safetensors",
        "weight_dtype": "default"
      }
    },
    "4": {
      "class_type": "CLIPLoader",
      "inputs": {
        "clip_name": "qwen_2.5_vl_7b_fp8_scaled.safetensors",
        "type": "qwen_image",
        "device": "default"
      }
    },
    "5": {
      "class_type": "LoraLoaderModelOnly",
      "inputs": {
        "model": ["3", 0],
        "lora_name": "qwen-image-edit-2511-multiple-angles-lora.safetensors",
        "strength_model": 0.9
      }
    },
    "6": {
      "class_type": "TextEncodeQwenImageEditPlus",
      "inputs": {
        "prompt": "PLACEHOLDER_PROMPT",
        "clip": ["4", 0],
        "vae": ["2", 0],
        "image1": ["1", 0]
      }
    },
    "7": {
      "class_type": "VAEEncode",
      "inputs": {
        "pixels": ["1", 0],
        "vae": ["2", 0]
      }
    },
    "8": {
      "class_type": "KSampler",
      "inputs": {
        "model": ["5", 0],
        "positive": ["6", 0],
        "negative": ["6", 0],
        "latent_image": ["7", 0],
        "seed": 12345,
        "steps": 30,
        "cfg": 5.0,
        "sampler_name": "euler",
        "scheduler": "simple",
        "denoise": 0.7
      }
    },
    "9": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["8", 0],
        "vae": ["2", 0]
      }
    },
    "10": {
      "class_type": "SaveImage",
      "inputs": {
        "images": ["9", 0],
        "filename_prefix": "qwen_lora_edit"
      }
    }
  }
}
